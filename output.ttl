        ns1:Cherry,
        ns1:Gin,
        ns1:Mint,
        ns1:Orange,
        ns1:Sugar ;
    ns1:hasRecipe "Muddle sugar with carbonated water and mint sprigs in an old-fashioned glass. Add gin and 1 ice cube. Stir, add the orange slice and the cherry, and serve." .

ns1:GinSour a ns1:OrdinaryDrink ;
    rdfs:label "Gin Sour" ;
    ns1:hasIngredient ns1:Gin,
        ns1:Lemonjuice,
        ns1:Maraschinocherry,
        ns1:Orange,
        ns1:Sugar ;
