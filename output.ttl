        ns1:Gin,
        ns1:Nutmeg,
        ns1:Port,
        ns1:Powderedsugar,
        ns1:Water ;
    ns1:hasRecipe "Dissolve powdered sugar in water and add gin. Pour into a highball glass over 2 ice cubes. Fill with carbonated water and stir. Float port on top, sprinkle lightly with nutmeg, and serve." .

ns1:GinSling a ns1:OrdinaryDrink ;
    rdfs:label "Gin Sling" ;
    ns1:hasIngredient ns1:Gin,
        ns1:Lemon,
        ns1:Orangepeel,
