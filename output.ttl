        ns1:Grapejuice,
        ns1:Lemonjuice ;
    ns1:hasRecipe "Place all ingredients in the blender jar - cover and whiz on medium speed until well blended. Pour in one tall, 2 medium or 3 small glasses and drink up." .

ns1:GrassSkirt a ns1:OrdinaryDrink ;
